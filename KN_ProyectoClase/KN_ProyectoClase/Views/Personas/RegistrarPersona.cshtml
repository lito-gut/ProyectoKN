@model KN_ProyectoClase.Models.PersonasModel

@{
    ViewBag.Title = "RegistrarPersona";
}

@using (Html.BeginForm("RegistrarPersona", "Personas"))
            {

                @Html.TextBoxFor(m => m.Identificacion, new { @class ="form-control"})
                @Html.TextBoxFor(m => m.Nombre, new { @class ="form-control" })
    
                <input type="submit" value="Registrar" class="btn btn-outline-success" />

            }